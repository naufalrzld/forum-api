{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "linebreak-style": 0,
      "eol-last": 0,
      "no-console": "off",
      "no-underscore-dangle": "off",
      "no-unused-vars": "off",
      "camelcase": "off",
      "class-methods-use-this": "off",
      "max-len": "off"
    }
}
